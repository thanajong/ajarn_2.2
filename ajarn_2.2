// BMI Calculator

#include<stdio.h>
#include<math.h>
int main()
{
    float w,h;
    printf("This is BMI Calculator, Enter your weight in kg. and your height in metres. : ");
	scanf("%f %f", &w, &h);
    
    float BMI = (round(w)/pow(h,2));
    if (BMI < 18.5)
    {
        printf("Underweight");
    }
    else if (BMI >= 18.5 && BMI <= 24.9)
    {
        printf("Normal");
    }
    else if (BMI >= 25.0 && BMI <= 29.9)
    {
        printf("Overweight");
    }
    else if (BMI > 30)
    {
        printf("Fat");
    }
    return 0;
}
